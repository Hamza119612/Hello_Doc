{% extends 'base.html.twig' %}

{% block title %}Spcialite index{% endblock %}

{% block body %}
    <h1>Spcialite index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Spec_Medcin</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for spcialite in spcialites %}
            <tr>
                <td>{{ spcialite.id }}</td>
                <td>{{ spcialite.SpecMedcin }}</td>
                <td>
                    <a href="{{ path('spcialite_show', {'id': spcialite.id}) }}">show</a>
                    <a href="{{ path('spcialite_edit', {'id': spcialite.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('spcialite_new') }}">Create new</a>
{% endblock %}
